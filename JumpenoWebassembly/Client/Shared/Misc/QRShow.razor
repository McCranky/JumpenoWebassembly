<img src="@Source" alt="QRcode" title="@Title" width="@Width" height="@Height" />

@code {
    [Parameter]
    public string Title { get; set; }
    [Parameter]
    public string Source { get; set; }
    [Parameter]
    public int Width { get; set; }
    [Parameter]
    public int Height { get; set; }
}
